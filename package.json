{
    "name": "mysite",
    "version": "0.1.0",
    "description": "description",
    "scripts": {
        "release": "run-s css pack",
        "pack": "webpack",
        "dev": "run-p serve sass-w",
        "serve": "webpack-dev-server --hot --inline",
        "sass-w": "nodemon -e .scss -x \"npm run sass\"",
        "sass": "node-sass ./_dev/scss/style.scss ./public/css/style.css --output-style expanded --source-map true",
        "css": "npm run sass && npm run prefix && npm run comb",
        "prefix": "postcss --use autoprefixer -b \"last 2 versions, Android >= 5.1, iOS >= 10\" ./public/css/style.css -o ./public/css/style.css",
        "comb": "csscomb ./public/css/style.css"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.3",
        "awesome-typescript-loader": "^3.2.3",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "csscomb": "^4.2.0",
        "file-loader": "^0.11.2",
        "node-sass": "^4.5.3",
        "nodemon": "^1.12.1",
        "npm-run-all": "^4.1.1",
        "postcss-cli": "^4.1.0",
        "sass-loader": "^5.0.1",
        "source-map-loader": "^0.2.1",
        "typescript": "^2.5.2",
        "webpack": "^3.5.5",
        "webpack-dev-server": "^2.8.2"
    },
    "dependencies": {
        "@types/gsap": "^1.19.1",
        "@types/three": "^0.84.20",
        "gsap": "^1.20.2",
        "three": "^0.87.1",
        "three-orbit-controls": "^82.1.0"
    },
    "author": "kato",
    "license": "ISC"
}
